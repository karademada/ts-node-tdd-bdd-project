{
  "name": "ts-node-tdd-bdd-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "test:unit": "jest --coverage --verbose --detectOpenHandles",
    "test:feature": "cucumber-js",
    "test:feature:pretty": "cucumber-js -- --format @cucumber/pretty-formatter",
    "test:all": "pnpm run test:unit && npm run test:feature",
    "test:watch:all": "tsc --watch & pnpm run test:all",
    "clean": "rimraf dist && rimraf coverage && rimraf node_modules/.cache",
    "pretest": "pnpm run clean && npm run build",
    "test:ci": "pnpm run test:all",
    "test:ci:debug": "pnpm run test:all -- --inspect-brk",
    "test:ci:watch": "pnpm run test:watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@types/cucumber": "^7.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "express": "^4.19.2"
  }
}
